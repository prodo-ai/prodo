{
  "name": "gatsby-plugin-prodo",
  "description": "Gatsby plugin to add support for prodo",
  "version": "0.1.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "clean": "echo 'clean not implemented'"
  },
  "homepage": "https://github.com/prodo-ai/prodo/tree/master/packages/gatsby-plugin#readme",
  "bugs": {
    "url": "https://github.com/prodo-ai/prodo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prodo-ai/prodo.git",
    "directory": "packages/gatsby-plugin"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prodo",
    "prodo-gatsby"
  ],
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "loader-utils": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "babel-preset-gatsby-package": "^0.2.7",
    "cross-env": "^5.2.1"
  },
  "peerDependencies": {
    "@prodo/babel-plugin": "*",
    "@prodo/core": "*"
  },
  "engines": {
    "node": ">=8.0.0"
